{
    "name": "katsana/geocoder",
    "description": "Geocoder tools for KATSANA development",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Geocoder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\Geocoder\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "orchestra/support": "^6.0",
        "php-http/guzzle7-adapter": "^1.0",
        "willdurand/geocoder": "^4.2"
    },
    "require-dev": {
        "geocoder-php/google-maps-provider": "^4.4",
        "geocoder-php/nominatim-provider": "^5.0",
        "orchestra/testbench": "^6.0"
    },
    "suggest": {
        "geocoder-php/google-maps-provider": "Use google_maps provider for reverse geolocation (^4.4).",
        "geocoder-php/nominatim-provider": "Use nominatim provider for reverse geolocation (^5.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
